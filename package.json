  "scripts": {
    "start": "standard --fix && standard && webpack-dev-server --config ./app/config/webpack/webpack.dev.config.js",
    "build": "webpack --config ./app/config/webpack/webpack.prod.config.js",
    "test": "jest --config config.json",
    "react-update-test": "yarn run test -- -u",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook",
    "start-docs": "cd app/e2e/publish && bower install && polymer serve --port 5005",
    "docs": "open -a \"Google Chrome\" './app/e2e/publish/output/initialize.md'",
    "e2etest": "mkdir -p screenShots && node ./app/e2e/gherkin-properties.js && wdio ./app/config/wdio/wdio.conf.js",
    "e2etest-ci": "NODE_ENV=ci wdio ./app/config/wdio/wdio.CI.conf.js",
    "e2etest-ci-headless": "NODE_ENV=ci-headless wdio ./app/config/wdio/wdio.CI-headless.conf.js",
    "e2etest-sit": "NODE_ENV=sit wdio ./app/config/wdio/wdio.SIT.conf.js",
    "sonar": "SONAR_SCANNER_OPTS=\"-Dproject.settings=sonar-scanner.properties\" node_modules/sonar-scanner/bin/sonar-scanner",
    "selenium-update": "./node_modules/selenium-standalone/bin/selenium-standalone install && rm -rf ./selenium && mkdir ./selenium && cp -f -R ./node_modules/selenium-standalone/.selenium/* ./selenium",
    "selenium-install": "rm -rf ./node_modules/selenium-standalone/.selenium && cp -f -R ./selenium ./node_modules/selenium-standalone/.selenium",
    "selenium": "selenium-standalone start",
    "reporter": "./app/e2e/buildreporter.sh",
    "mb-configure": "node ./app/config/mountebank/mountebank.conf.js",
    "mb-start-docker": "docker pull dcregistry.service.dev:5000/mountebank && docker run -p 2525:2525 -p 4545:4545 -d dcregistry.service.dev:5000/mountebank && yarn mb-configure",
    "translate": "./node_modules/.bin/babel app/src --out-dir fsrc --ignore assets,e2e"
  },
